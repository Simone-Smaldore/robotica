function [] = stampaGrafico(parametri, tempi)
    for i = 1: size(tempi, 2) - 1
        disp("PORCO");
        t = linspace(tempi(i), tempi(i + 1)); 
        %plot(t, polyval(parametri(i, :), t));  
        plot(t, polyval(parametri(i, :), t));
        hold on;
    end 
    %B = [linspace(0, 1),polyval(parametri(1, :), linspace(0, 1)) , linspace(1, 2),polyval(parametri(2, :), linspace(1, 2))];
    %B = mat2cell(B,1,ones(1,numel(B)))
    %plot(B{});
end

